{
  "mcpServers": {
    "openapi-korea": {
      "command": "python",
      "args": [
        "your_project_directory/openapi_korea/src/openapi_korea/server.py"
      ],
      "env": {
        "OPENAPI_KOREA_SERVICE_KEY": "your_openapi_korea_service_key",
        "OPENAPI_RESOURCE_FILE_PATH": "your_file_path"
      },
      "disabled": false,
      "autoApprove": ["search_data", "show_cached_data", "refresh_data"]
    }
  }
}
